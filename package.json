{
  "name": "wai-website",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build --remote",
    "preview": "astro preview",
    "astro": "astro",
    "prettier": "prettier --write **/*.{ts,tsx,css,astro} --ignore-path .gitignore",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/db": "^0.17.1",
    "@astrojs/markdown-remark": "^6.3.5",
